{
  "editor.unicodeHighlight.nonBasicASCII": "inUntrustedWorkspace",
  "i18n-ally.localesPaths": [
    "public/locales",
    "src/shared/config/i18n"
  ],
  "stylelint.enable": true,
  "css.validate": true,
  "scss.validate": true,
  "stylelint.validate": [
    "css",
    "scss"
  ],
  // "scss.validate": false,
  // "css.validate": false,
  // "less.validate": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.stylelint": true
  },
}
